        <inactive flag="MensajeVisto_Problema2" />
      </condition>
      <speak-char idTarget="Abuelo">
        <resources name="No name" />
        <text>¿Cómo debería proceder?</text>
      </speak-char>
      <condition>
        <inactive flag="MensajeVisto_Problema2" />
      </condition>
      <child nodeindex="1" />
    </dialogue-node>
    <option-node nodeindex="1" preListening="yes" editor-x="690" editor-y="455" x="690" y="455">
